[run]
source = src/copilot_money_mcp
branch = True
omit =
    */tests/*
    */__pycache__/*
    */.venv/*
    */venv/*
    src/copilot_money_mcp/__main__.py

[report]
precision = 2
show_missing = True
skip_covered = False
fail_under = 90.0
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
